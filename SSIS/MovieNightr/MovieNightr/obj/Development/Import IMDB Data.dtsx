                else
                {

                    if (_name == "")
                    {
                        _name = Row.Data.Substring(0, Row.Data.IndexOf('\t'));
                    }

                    movie = Row.Data.Replace(_name, "").Replace("\t", "");
                    if (movie.Contains("{"))
                    {
                        movieString = movie.Substring(0, movie.LastIndexOf("{")).Trim();
                        Row.Role = movie.Substring(movie.LastIndexOf("{"));
