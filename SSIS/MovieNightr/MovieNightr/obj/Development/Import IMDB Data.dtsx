                    if (scrub.Success)
                    {
                        Row.ScrubbedMovie = movie.Substring(0, scrub.Index + scrub.Length);
                    }
                    else
                    {
                        Row.ScrubbedMovie = movieString;
                    }
                }
            }

        }
        catch (Exception ex)
